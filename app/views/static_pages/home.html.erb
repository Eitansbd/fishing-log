<% provide(:title, "Home") %>

  <div class="row">
    <% unless logged_in? %>
      <%= link_to "Click here to be logged in as a user and explore the application", demo_login_path, class: "btn btn-primary pull-right", method: :post %>
      <% end %>
    <div class="col-md-6 recent-trips">
      <h1 class="heading">The Fishing Report...</h1>
        <%= render @fishing_trips %>
    </div>
    <div class="col-md-5 col-md-offset-1">
      <%= render 'fish_catches/fish_table' %>
    </div>
  </div>
  
 


  
  
